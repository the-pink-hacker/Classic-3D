{
    "multipart": [
		{   "apply": { "model": "block/iron_bars_post" }},
        {   "when": { "north": true },
            "apply": { "model": "block/iron_bars_side_2" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/iron_bars_side_2", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/iron_bars_side_alt_2" }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/iron_bars_side_alt_2", "y": 90 }
        }
    ]
}
