{
  "type": "multi",
  "patch": [
    {
      "type": "mixin_json",
      "patch": {
        "mixins": [
          {
            "file_selector": {
              "type": "path",
              "arguments": {
                "path": "assets/minecraft/models/*"
              }
            },
            "selector": {
              "type": "path",
              "arguments": {
                "location": "textures/*"
              }
            },
            "modifiers": [
              {
                "type": "replace",
                "arguments": {
                  "select": "^block|(?<=^minecraft:)block",
                  "replacement": "blocks"
                }
              }
            ]
          },
          {
            "file_selector": {
              "type": "path",
              "arguments": {
                "path": "assets/minecraft/blockstates"
              }
            },
            "selector": {
              "type": "path",
              "arguments": {
                "location": "variants/*/model"
              }
            },
            "modifiers": [
              {
                "type": "replace",
                "arguments": {
                  "select": "^block/|(?<=^minecraft:)block/",
                  "replacement": ""
                }
              }
            ]
          },
          {
            "file_selector": {
              "type": "path",
              "arguments": {
                "path": "assets/minecraft/blockstates"
              }
            },
            "selector": {
              "type": "path",
              "arguments": {
                "location": "multipart/*/apply/model"
              }
            },
            "modifiers": [
              {
                "type": "replace",
                "arguments": {
                  "select": "^block/|(?<=^minecraft:)block/",
                  "replacement": ""
                }
              }
            ]
          }
        ]
      }
    }
  ]
}