{
  "$schema": "https://raw.githubusercontent.com/RyanGar46/resource-pack-packer/main/schema/patch.schema.json",
  "patches": [
    {
      "type": "modifier",
      "patch": {
        "type": "model_margin",
        "arguments": {
          "offset": -0.01,
          "random_offset": -0.001,
          "file_selector": {
            "type": "identifier",
            "arguments": {
              "models": [
                "minecraft:block/template_cobblestone",
                "minecraft:block/template_sandstone",
                "minecraft:block/template_nether_bricks",
                "minecraft:block/template_stone_bricks",
                "minecraft:block/template_planks",
                "minecraft:block/prismarine",
                "minecraft:block/bricks"
              ]
            }
          }
        }
      }
    }
  ]
}